{
    "name": "marshmallow/nova-map-box-field",
    "description": "A field for Laravel Nova 4 to create mapbox polygons and store them into your database. Awesome stuff really!",
    "keywords": [
        "laravel",
        "nova",
        "map box"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\MapBox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\MapBox\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
